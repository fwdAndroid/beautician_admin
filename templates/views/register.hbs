<body class="font-montserrat">

<div class="auth">
    <div class="auth_left">
        <div class="card">
            <div class="text-center mb-5">
               <i class="fa fa-soccer-ball-o brand-logo"></i>
            <div class="card-body">
                <div class="card-title">Create new account</div>
                <div class="form-group">
                    <label class="form-label">Name</label>
                    <input type="text" class="form-control" placeholder="Enter name">
                </div>
                <div class="form-group">
                    <label class="form-label">Email address</label>
                    <input type="email" class="form-control" placeholder="Enter email">
                </div>
                <div class="form-group">
                    <label class="form-label">Password</label>
                    <input type="password" class="form-control" placeholder="Password">
                </div>
                <div class="form-footer">
                    <button type="submit" class="btn btn-primary btn-block" id="/register">Create new account</button>
                </div>
            </div>
            <div class="text-center text-muted">
                Already have account? <a href="/">Sign in</a>
            </div>
        </div>
    </div>
    <div class="auth_right full_img"></div>
</div>

<script src="bundles/lib.vendor.bundle.js"></script>
<script src="js/core.js"></script>
